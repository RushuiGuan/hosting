{
	"$schema": "schema-appsettings.json",
	"angular": {
		"configFile": [
			"appsettings.json",
			"appsettings.Development.json"
		],
		"requestPath": "/configuration",
		"baseHref": "/",
		"baseHrefFile": [
			"wwwroot/index.html"
		]
	},
	"authentication": {
		"useKerberos": false,
		"default": "Google",
		"bearerTokens": [
			{
				"audience": [
					"https://example.com",
					"https://api.example.com"
				],
				"authority": "https://accounts.google.com",
				"issuer": "https://accounts.google.com",
				"provider": "Google",
				"requireHttpsMetadata": false,
				"validateAudience": true,
				"validateIssuer": true,
				"validateLifetime": true
			}
		]
	}
}
